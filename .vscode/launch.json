{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Blinky (ST-Link)",
            "type": "cortex-debug",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceFolder}/builds/build_blinky/executables/blinky.elf",
            "servertype": "stlink",
            "serverpath": "${env:STM32CLT_PATH}/STLink-gdb-server/bin/ST-LINK_gdbserver.exe",
            "stm32cubeprogrammer": "${env:STM32CLT_PATH}/STM32CubeProgrammer/bin",
            "device": "STM32G474RE",
            "interface": "swd",
            "serialNumber": "",
            "runToEntryPoint": "main",
            "svdFile": "${env:STM32CLT_PATH}/STMicroelectronics_CMSIS_SVD/STM32G474.svd",
            "preLaunchTask": "Build Blinky",
            "showDevDebugOutput": "both"
        },
        {
            "name": "Debug Firmware (ST-Link)",
            "type": "cortex-debug",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceFolder}/builds/build_firmware/executables/firmware.elf",
            "servertype": "stlink",
            "serverpath": "${env:STM32CLT_PATH}/STLink-gdb-server/bin/ST-LINK_gdbserver.exe",
            "stm32cubeprogrammer": "${env:STM32CLT_PATH}/STM32CubeProgrammer/bin",
            "device": "STM32G474RE",
            "interface": "swd",
            "serialNumber": "",
            "runToEntryPoint": "main",
            "svdFile": "${env:STM32CLT_PATH}/STMicroelectronics_CMSIS_SVD/STM32G474.svd",
            "preLaunchTask": "Build Firmware",
            "showDevDebugOutput": "none"
        },
        {
            "name": "Attach to Target (ST-Link)",
            "type": "cortex-debug",
            "request": "attach",
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceFolder}/builds/build_blinky/executables/blinky.elf",
            "servertype": "stlink",
            "serverpath": "${env:STM32CLT_PATH}/STLink-gdb-server/bin/ST-LINK_gdbserver.exe",
            "stm32cubeprogrammer": "${env:STM32CLT_PATH}/STM32CubeProgrammer/bin",
            "device": "STM32G474RE",
            "interface": "swd",
            "svdFile": "${env:STM32CLT_PATH}/STMicroelectronics_CMSIS_SVD/STM32G474.svd",
            "showDevDebugOutput": "none"
        }
    ]
}
